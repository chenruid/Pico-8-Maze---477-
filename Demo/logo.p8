pico-8 cartridge // http://www.pico-8.com
version 36
__lua__
fcount = 0

logoanim=1
logostop=31
logofr=70
function logo()
 local cx = 55
 local cy = 45
 local frx=logofr%16*8
 local fry=flr(logofr/16)*8
 for s=0,logoanim do
  for x=0,15 do
   camera(rnd(30/fcount),
          rnd(30/fcount))
   for y=0,15 do
    if(x+y==s) then 
     pset(cx+x,cy+y,7)
    elseif(x+y == s-1) then
     pset(cx+x,cy+y,6)
    elseif(x+y<s-1) then
     pset(cx+x,cy+y,
      sget(frx+x,fry+y))
    end
   end
  end
 end
 if(logoanim<=logostop) logoanim+=1
 if(fcount > 35) then
  print("mazeindungeon",36,cy+20,9)
  print("carts",53,cy+26,8)
 end
end

function _update()
 fcount += 1
end

function _draw()
 cls()
 logo()
end
